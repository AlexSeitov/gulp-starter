{
  "name": "gulp-starter",
  "version": "1.0",
  "description": "gulp starter for layout websites",
  "type": "module",
  "scripts": {
    "dev": "gulp dev && webpack serve --config ./webpack.dev.js",
    "start": "gulp start",
    "build": "gulp build --build",
    "start:build": "gulp startBuild",
    "htmlhint": "npx htmlhint ./src/html/*",
    "htmlValidation": "gulp htmlValidation",
    "stylelint": "stylelint './**/*.scss'",
    "stylelint:fix": "stylelint --fix './**/*.scss'",
    "lint": "prettier --check './**/*.(scss|js)'",
    "prettier:fix": "prettier --write './**/*.(scss|js)'",
    "eslint": "eslint './**/*.js'",
    "eslint:fix": "eslint --fix './**/*.js'",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 2 years",
    "> 1%",
    "not dead",
    "not ie 11"
  ],
  "keywords": [],
  "author": "Alexander Seitov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.0",
    "browser-sync": "^2.27.10",
    "css-loader": "^6.7.2",
    "del": "^7.0.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "file-include-webpack-plugin": "^1.3.9",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-favicons": "^3.0.0",
    "gulp-file-include": "^2.3.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-html-beautify": "^1.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "^5.1.0",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-w3c-html-validator": "^5.1.3",
